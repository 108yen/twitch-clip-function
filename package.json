{
  "name": "twitch-clip-function",
  "version": "1.4.8",
  "private": "true",
  "packageManager": "pnpm@10.12.4",
  "scripts": {
    "prepare": "lefthook install",
    "build": "tsc",
    "start": "node .",
    "test": "jest --runInBand",
    "test:silent": "jest --runInBand --silent",
    "test:ci": "firebase emulators:exec 'pnpm jest --runInBand --silent --coverage' --project=twitch-clip-dev --only firestore --import test/test_data/firestore",
    "firestore:emulators": "firebase emulators:start --project=twitch-clip-dev --only firestore --import test/test_data/firestore",
    "lint": "eslint . --max-warnings=0 --cache",
    "lint:inspect": "pnpm dlx @eslint/config-inspector",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check --cache .",
    "format:write": "prettier --write --cache .",
    "quality": "pnpm format:check && pnpm lint && pnpm typecheck",
    "test:update-data": "tsx scripts/update-test-data.ts"
  },
  "main": "dist/src/index.js",
  "dependencies": {
    "axios": "^1.9.0",
    "dayjs": "^1.11.13",
    "dotenv": "^17.0.0",
    "firebase-admin": "^13.4.0",
    "loader": "^2.1.1",
    "twitter-api-v2": "^1.23.2"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.29.4",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@cspell/eslint-plugin": "^9.0.2",
    "@eslint/compat": "^1.2.9",
    "@eslint/js": "^9.27.0",
    "@faker-js/faker": "^9.8.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.15.21",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "@typescript-eslint/utils": "^8.32.1",
    "@typescript/native-preview": "7.0.0-dev.20250711.1",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^4.3.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-perfectionist": "^4.13.0",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-typegen": "^2.2.0",
    "globals": "^16.1.0",
    "jest": "^30.0.0",
    "jiti": "^2.4.2",
    "lefthook": "^1.11.13",
    "prettier": "^3.5.3",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1"
  }
}
