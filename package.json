{
  "name": "twitch-clip-function",
  "version": "1.4.11",
  "private": "true",
  "packageManager": "pnpm@10.30.1",
  "scripts": {
    "prepare": "lefthook install",
    "build": "tsc --module commonjs",
    "start": "node .",
    "test": "vitest run",
    "test:ui": "vitest --ui --coverage.enabled=true",
    "test:coverage": "vitest run --coverage",
    "test:ci": "firebase emulators:exec 'pnpm test:coverage' --project=twitch-clip-dev --only firestore --import test/test_data/firestore",
    "firestore:emulators": "firebase emulators:start --project=twitch-clip-dev --only firestore --import test/test_data/firestore",
    "lint": "eslint . --max-warnings=0 --cache",
    "lint:inspect": "pnpm dlx @eslint/config-inspector",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check --cache .",
    "format:write": "prettier --write --cache .",
    "quality": "pnpm format:check && pnpm lint && pnpm typecheck",
    "test:update-data": "tsx scripts/update-test-data.ts"
  },
  "main": "dist/src/index.js",
  "dependencies": {
    "axios": "^1.13.2",
    "dayjs": "^1.11.19",
    "dotenv": "^17.2.3",
    "firebase-admin": "^13.6.0",
    "loader": "^2.1.1",
    "twitter-api-v2": "^1.28.0"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.2",
    "@changesets/cli": "^2.29.8",
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@cspell/eslint-plugin": "^9.4.0",
    "@eslint/compat": "^2.0.0",
    "@eslint/js": "^9.39.1",
    "@faker-js/faker": "^10.1.0",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "@typescript-eslint/utils": "^8.48.1",
    "@typescript/native-preview": "7.0.0-dev.20260124.1",
    "@vitest/coverage-v8": "^4.0.15",
    "@vitest/eslint-plugin": "^1.5.1",
    "@vitest/ui": "^4.0.15",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-perfectionist": "^5.0.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-unused-imports": "^4.3.0",
    "eslint-typegen": "^2.3.0",
    "globals": "^17.0.0",
    "jiti": "^2.6.1",
    "lefthook": "^2.0.8",
    "prettier": "^3.7.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1",
    "vite": "^7.2.6",
    "vitest": "^4.0.15"
  }
}
