{
  "name": "twitch-clip-function",
  "version": "1.4.5",
  "private": "true",
  "packageManager": "pnpm@10.11.0",
  "scripts": {
    "prepare": "lefthook install",
    "build": "tsc",
    "start": "node .",
    "test": "jest --runInBand",
    "test:silent": "jest --runInBand --silent",
    "test:ci": "firebase emulators:exec 'pnpm jest --runInBand --silent --coverage' --project=twitch-clip-dev --only firestore --import test/test_data/firestore",
    "firestore:emulators": "firebase emulators:start --project=twitch-clip-dev --only firestore --import test/test_data/firestore",
    "lint": "eslint . --max-warnings=0 --cache",
    "lint:inspect": "pnpm dlx @eslint/config-inspector",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check --cache .",
    "format:write": "prettier --write --cache .",
    "quality": "pnpm format:check && pnpm lint && pnpm typecheck",
    "test:update-data": "tsx scripts/update-test-data.ts"
  },
  "main": "dist/src/index.js",
  "dependencies": {
    "axios": "^1.3.4",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "firebase-admin": "^13.0.0",
    "loader": "^2.1.1",
    "twitter-api-v2": "^1.15.2"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.9",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@cspell/eslint-plugin": "^9.0.0",
    "@eslint/compat": "^1.2.0",
    "@eslint/js": "^9.25.1",
    "@faker-js/faker": "^9.0.0",
    "@types/jest": "^29.5.3",
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@typescript-eslint/utils": "^8.32.1",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^4.0.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-perfectionist": "^4.0.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-typegen": "^2.2.0",
    "globals": "^16.0.0",
    "jest": "^29.6.1",
    "jiti": "^2.3.3",
    "lefthook": "^1.7.18",
    "prettier": "^3.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.8.1"
  }
}
